def main():
    testfunc(42)
    passFunc()

# optional arguements have default values
def testfunc(number, another = 4, yetAnother = 5):
    print('This is a test function', number)

# this function serves as a stub to be filled in later
def passFunc():
    pass

if __name__ == "__main__": main()
